<launch>   
   <node pkg="sampling_core" type="voronoi_test_node" name="voronoi_test" output="screen" >
         <rosparam>
            num_robot: 3
            x_range: [0, 100]
            y_range: [0, 100]
            resolution: 1.0
            hetero_index: [0, 1, 2]
            scale_factor: [1.0, 1.0, 1.0]
            motion_primitives:
                - param: [1.0, 0.5, 1.0]
                - param: [1.0, 2.0, 1.0]
                - param: [1.0, 1.0, 1.0]
         </rosparam>
   </node>
</launch>