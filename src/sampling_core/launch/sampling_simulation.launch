<launch>
  <node pkg="sampling_core" type="sampling_simulation_node" name="sampling_simulation" output="screen"> 
    <rosparam>
            num_agents: 3
            model_parameters:
                - num_gaussian: 3
                  param0: [0.5, 0.5, 0.1]
                  param1: [0.5, 0.5, 0.1]
                  param2: [0.5, 0.5, 0.1]
                  max_iteration: 100
                  eps: 0.1
            data_path:
                - test_location: "wifi_pos_new_2router_0.txt"
                  initial_measurements: "wifi_measurement_new_2router_0.txt"
                  initial_locations: "wifi_pos_new_2router_0.txt"
            sampling_parameters:
                - selection_model: 1
                  variance_coef: 0.5
            graph_visualization_parameters:
                - map_frame: "mean"
                  map_id: 1
                  x_scale: 1.0
                  y_scale: 1.0
                  x_offset: 0.0
                  y_offset: 0.0
                  lower_bound: 0.0
                  upper_bound: 1.5
                - map_frame: "variance"
                  map_id: 2
                  x_scale: 1.0
                  y_scale: 1.0
                  x_offset: 15.0
                  y_offset: 0.0
                  lower_bound: 0.0
                  upper_bound: 1.0
            robot_visualization_parameter:
                - map_frame: "robot"
                  map_id: 3
                  x_scale: 5.0
                  y_scale: 5.0
                  x_offset: 0.0
                  y_offset: 0.0
                  lower_bound: 0.0
                  upper_bound: 1.0
         </rosparam>
  </node>

</launch>