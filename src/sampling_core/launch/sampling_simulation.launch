<launch>
  <node pkg="sampling_core" type="sampling_simulation_node" name="sampling_simulation" output="screen"> 
    <rosparam>
            num_agents: 2
            model_parameters:
                - num_gaussian: 2
                  param0: [0.5, 0.5, -2.0]
                  param1: [0.5, 0.5, -2.0]
                  param2: [0.5, 0.5, -2.0]
                  param3: [0.5, 0.5, -2.0]
                  max_iteration: 100
                  eps: 0.1
            data_path:
                - test_location: "gt_final_fake_GPS.txt"
                  initial_measurements: "init_final_fake_temperature.txt"
                  initial_locations: "init_final_fake_GPS.txt"
                  gt_measurements: "gt_final_fake_temperature.txt"
                  gt_locations: "gt_final_fake_GPS.txt"
                  traj_0: "robot_0_traj.txt"
                  traj_1: "robot_1_traj.txt"
                  traj_follow: False
                  traj_rotate_angle: 0.41450687
                  traj_lat_offset: -3.8
                  traj_lng_offset: 1.0
            sampling_parameters:
                - selection_model: 1
                  variance_coef: 12.0
            graph_visualization_parameters:
                - map_frame: "mean"
                  map_id: 1
                  x_scale: 1.0
                  y_scale: 1.0
                  x_offset: 30.0
                  y_offset: 0.0
                  lower_bound: 0.0
                  upper_bound: 15.0
                - map_frame: "variance"
                  map_id: 2
                  x_scale: 1.0
                  y_scale: 1.0
                  x_offset: 60.0
                  y_offset: 0.0
                  lower_bound: 0.0
                  upper_bound: 2.0 #15 for gt
            robot_visualization_parameter:
                - map_frame: "robot"
                  map_id: 3
                  x_scale: 2.0
                  y_scale: 2.0
                  x_offset: 0.0
                  y_offset: 0.0
                  lower_bound: 0.0
                  upper_bound: 1.0
            voronoi_visualization_parameter:
                - map_frame: "voronoi"
                  map_id: 4
                  x_scale: 1.0
                  y_scale: 1.0
                  x_offset: 0.0
                  y_offset: 0.0
                  lower_bound: 0.0
                  upper_bound: 1.0
            heterogeneous_parameter:
                - hetero_spaces: [0, 1, 2, 4]
                # - hetero_spaces: [0]
                  hetero_scale_factors: [ 1.5, 2.0, 1.0, 1.0]
                  mp0: [ 1.0, 0.1, 1.25, 1.0]
                  mp1: [ 1.0, 0.5, 0.75, 1.0]
                  mp2: [ 1.0, 5.0, 0.3, 1.0]
                  mp3: [ 1.0, 5.0, 0.3, 1.0]
                  poly_coeff: [-5.120129, 7.906774, 9.684723, -0.136121, -8.459288,
                  -4.805916, -1.322104, 2.407984, 2.038850, 1.046061, 
                  0.318280, -0.238998, -0.275353, -0.242692, -0.099094, 
                  -0.020971, 0.002012, 0.022830, 0.003186, 0.012989, 0.003254]
            obstacle_list:
                - agent_id: 0
                  # obstacle_path: "obstacle_1.txt"
                - agent_id: 1
                  obstacle_path: "obstacle_1.txt"
         </rosparam>
  </node>

</launch>